{
  "name": "solscan-js",
  "version": "0.1.63",
  "description": "TypeScript client for Solscan API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:token": "jest tests/tokenApi.test.ts",
    "test:transaction": "jest tests/transactionApi.test.ts",
    "test:account": "jest tests/accountApi.test.ts",
    "test:all": "jest tests/",
    "test:coverage": "jest tests/ --coverage",
    "test:watch": "jest tests/ --watch",
    "example": "ts-node  example.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "solana",
    "solscan",
    "api",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srpiroliro/solscan.js.git"
  },
  "author": "srpiroliro",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "dotenv": "^16.5.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2"
  }
}
