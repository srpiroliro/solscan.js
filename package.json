{
  "name": "solscan-js",
  "version": "0.1.4",
  "description": "TypeScript client for Solscan API",

  "main": "dist/index.js",
  "types": "dist/index.d.ts",

  "exports": {
     ".": "./dist/index.js",
     "./*": "./dist/*.js"
   },

   "typesVersions": {
     "*": {
       "*": ["dist/*"]
     }
   },

  "scripts": {
    "build": "tsc",
    "test": "jest",
    "publish": "npm run build && npm publish",
    "example": "ts-node  example.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "solana",
    "solscan",
    "api",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srpiroliro/solscan.js.git"
  },
  "author": "srpiroliro",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2"
  }
}
